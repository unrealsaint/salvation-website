{
    "version": 2,
    "builds": [
        {
            "src": "**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/features",
            "dest": "/features.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}